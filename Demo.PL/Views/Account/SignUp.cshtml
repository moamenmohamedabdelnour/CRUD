@model SignUpViewModel
@{
    ViewData["Title"] = "SignUp";
    Layout = "~/Views/Shared/_AuthLayout.cshtml";
}

<div class="main-agileinfo">
	<div class="agileits-top">
		<form asp-action="SignUp" method="post">
			<div asp-validation-summary="All"></div>
@*			<input class="text" type="text" name="Username" placeholder="Username" required="">*@
			<input class="text" asp-for="FName" placeholder="FirstName" required="">
			<input class="text" asp-for="LName" placeholder="Last Name" required="">
			<input class="text" asp-for="UserName" placeholder="Username" required="">
			<input class="text email" asp-for="Email" placeholder="Email" required="">
			<input class="text" asp-for="Password"  placeholder="Password" required="">
			<input class="text w3lpass" asp-for="ConfirmPassword" placeholder="Confirm Password" required="">
			<div class="wthree-text">
				<label class="anim">
					<input asp-for="IsAgree" class="checkbox" required="">
					<span>I Agree To The Terms & Conditions</span>
				</label>
				<div class="clear"> </div>
			</div>
			<input type="submit" value="SIGNUP">
		</form>
		<p>Do you have an Account? <a asp-action="SignIn"> Login Now!</a></p>
	</div>
</div>
@section ValidationScriptSection{
	<partial name="_ValidationScriptsPartial"></partial>
	@*<script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
	<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>*@
}